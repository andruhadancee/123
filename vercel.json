{
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "devCommand": null,
  "framework": null,
  "crons": [
    {
      "path": "/api/archive-auto",
      "schedule": "59 23 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/teams",
      "destination": "/teams.html"
    },
    {
      "source": "/archive",
      "destination": "/archive.html"
    },
    {
      "source": "/calendar",
      "destination": "/calendar.html"
    },
    {
      "source": "/regulations",
      "destination": "/regulations.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
